/*
   2L system of Hogeweg and Hesper
   Figure 1.31a pg. 35
 */
#define maxgen 30
#define delta 22.5
#ignore +-F

START : F 1 F 1 F 1

p1 : 0 < 0 > 0 -> 0
p2 : 0 < 0 > 1 -> 1[+F 1 F 1]
p3 : 0 < 1 > 0 -> 1
p4 : 0 < 1 > 1 -> 1
p5 : 1 < 0 > 0 -> 0
p6 : 1 < 0 > 1 -> 1 F 1
p7 : 1 < 1 > 0 -> 0
p8 : 1 < 1 > 1 -> 0
p9 :	 +     -> -
p10:	 -     -> +
