/*
   Compound leaf.
   Figure 5.11b pg. 129
 */

#define maxgen 14   /* not 16 as in text */
#define delta  45

#define D 1	/* apical delay */
#define R 1.5	/* internode elongation rate */

START : A(0)

p1 : A(d) : d > 0 -> A(d-1)
p2 : A(d) : d = 0 -> F(1)[+A(D)][-A(D)]F(1)A(0)
p3 : F(a)	  -> F(a*R)
